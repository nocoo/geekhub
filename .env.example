# DEV_MODE_ENABLED=true
# NEXT_PUBLIC_DEV_MODE_ENABLED=true
# DEV_USER_ID=your_user_id_here
# NEXT_PUBLIC_DEV_USER_ID=your_user_id_here
# DEV_USER_EMAIL=your_email_here
# NEXT_PUBLIC_DEV_USER_EMAIL=your_email_here
